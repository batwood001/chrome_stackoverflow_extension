 {
  "name": "Toggle Stackoverflow sidebar",
  "manifest_version": 2,
  "version": "1.2",
  "description": "Adds a button to toggle the Stackoverflow sidebar.",
  "page_action": {
    "name": "Toggle Sidebar"
  },
  "icons": {
    "128": "icon.png",
    "48": "icon.png",
    "16": "icon.png"
  },
  "content_scripts": [ {
    "css": ["jquery-ui.css"],
    "js": [ "jquery.min.js", "background.js", "jquery-ui.min.js"],
    "matches": [ "*://*.stackoverflow.com/*"]
  }]
}